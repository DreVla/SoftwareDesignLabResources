<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="146.0" prefWidth="185.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.utcluj.controller.LoginController">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            <TextField fx:id="username" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            <Button fx:id="login" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Login"
                    textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        </children>
    </GridPane>
</AnchorPane>
